#!/home/kdubs/cours_devops-estia/mkdocs-documentation/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'watchdog==2.1.9','console_scripts','watchmedo'
__requires__ = 'watchdog==2.1.9'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('watchdog==2.1.9', 'console_scripts', 'watchmedo')()
    )
